{
	"name": "@lpdsgn/gsap-spa-manager",
	"version": "0.1.0",
	"description": "Framework-agnostic GSAP animation manager for SPAs with automatic cleanup, persistence, and ScrollTrigger support",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./adapters/swup": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./adapters/barba": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"browser": "./dist/index.umd.js",
	"unpkg": "./dist/index.umd.js",
	"jsdelivr": "./dist/index.umd.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"prepublishOnly": "pnpm build"
	},
	"keywords": [
		"gsap",
		"animation",
		"spa",
		"swup",
		"barba",
		"scrolltrigger",
		"animation-manager",
		"cleanup",
		"vanilla-js",
		"typescript"
	],
	"author": "",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LPdsgn/gsap-spa-manager.git"
	},
	"bugs": "https://github.com/LPdsgn/gsap-spa-manager/issues",
	"homepage": "https://github.com/LPdsgn/gsap-spa-manager#readme",
	"license": "MIT",
	"peerDependencies": {
		"gsap": "^3.14.2"
	},
	"peerDependenciesMeta": {
		"gsap": {
			"optional": false
		}
	},
	"optionalDependencies": {},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"gsap": "^3.14.2",
		"swup": "^4.0.0",
		"tsup": "8.5.1",
		"typescript": "^5.0.0"
	}
}
